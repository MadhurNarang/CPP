//CHECK WHETHER NUMBER IS PALINDROME OR NOT
#include<iostream.h>
void palindrome(int a)
{
  int b=0,c,d;
  c=a;
  while(c!=0)
  {
	d=c%10;
	b=(b*10)+d;
	c/=10;
  }
  if(b==a)
	 cout<<"\n\nENTERED NUMBER IS PALINDROME";
  else
	 cout<<"\n\nENTERED NUMBER IS NOT A PALINDROME";
}
void main()
{
  int a;
  cout<<"ENTER THE NUMBER TO CHECK FOR PALINDROME     ";
  cin>>a;
  palindrome(a);
}
